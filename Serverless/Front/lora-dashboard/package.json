{
  "name": "@serverless/front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --ssl --configuration development",
    "start:local": "ng serve --configuration local --open",
    "start:local-token": "ng serve --configuration local --open",
    "start:runpod": "ng serve --configuration runpod --open",
    "start:mock": "ng serve --configuration development --open",
    "build": "ng build",
    "build:local": "ng build --configuration local",
    "build:runpod": "ng build --configuration runpod",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "dev": "npm run start:local",
    "dev:runpod": "npm run start:runpod",
    "dev:mock": "npm run start:mock"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "17.3.12",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "17.3.12",
    "@angular/compiler": "17.3.12",
    "@angular/core": "17.3.12",
    "@angular/forms": "17.3.12",
    "@angular/material": "^17.3.10",
    "@angular/platform-browser": "17.3.12",
    "@angular/platform-browser-dynamic": "17.3.12",
    "@angular/router": "17.3.12",
    "crypto-js": "^4.2.0",
    "js-yaml": "^4.1.0",
    "monaco-editor": "^0.45.0",
    "ngx-monaco-editor-v2": "^17.0.0",
    "rxjs": "7.8.1",
    "tailwindcss": "^3.4.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.3.11",
    "@angular/cli": "~17.3.12",
    "@angular/compiler-cli": "17.3.12",
    "@types/crypto-js": "^4.2.2",
    "@types/jasmine": "~5.1.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^18.16.9",
    "autoprefixer": "^10.4.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
